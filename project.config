[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group Ocean Observatories Owners
[access "refs/for/refs/*"]
	read = group user/Pete Cable (petercable)
	push = group user/Pete Cable (petercable)
	label-Code-Review = -2..+2 group user/Pete Cable (petercable)
	label-Verified = -1..+1 group user/Pete Cable (petercable)
	rebase = group user/Pete Cable (petercable)
	removeReviewer = group user/Pete Cable (petercable)
	submit = group user/Pete Cable (petercable)
[access "refs/heads/*"]
	read = group user/Pete Cable (petercable)
	create = group user/Pete Cable (petercable)
	push = +force group user/Pete Cable (petercable)
	pushMerge = group user/Pete Cable (petercable)
	label-Code-Review = -2..+2 group Instrument Approvers
	label-Verified = -1..+1 group Instrument Approvers
	submit = group Instrument Approvers
[access "refs/tags/*"]
	push = group Ocean Observatories Owners
	pushTag = group Ocean Observatories Owners
	pushSignedTag = group Ocean Observatories Owners
[access "refs/heads/unstreamed"]
	label-Code-Review = -2..+2 group SE_Unstreamed
	label-Verified = -1..+1 group SE_Unstreamed
	submit = group SE_Unstreamed
